                <!-- CSS スタイリングガイド -->
                <mxCell id="stylingGuide" value="CSS スタイリングガイド:&lt;br&gt;&lt;br&gt;.feature-card {&lt;br&gt;  border: 2px solid #dee2e6;&lt;br&gt;  background: #f8f9fa;&lt;br&gt;  border-radius: 4px;&lt;br&gt;  padding: 8px;&lt;br&gt;}&lt;br&gt;&lt;br&gt;.user-story.expanded {&lt;br&gt;  /* 展開時のスタイル */&lt;br&gt;}&lt;br&gt;&lt;br&gt;.user-story.collapsed {&lt;br&gt;  height: 25px;&lt;br&gt;  overflow: hidden;&lt;br&gt;}&lt;br&gt;&lt;br&gt;.collapse-btn {&lt;br&gt;  width: 15px;&lt;br&gt;  height: 15px;&lt;br&gt;  border: 1px solid #e0e0e0;&lt;br&gt;  background: #ffffff;&lt;br&gt;  font-size: 8px;&lt;br&gt;}&lt;br&gt;&lt;br&gt;.assignee {&lt;br&gt;  font-size: 7px;&lt;br&gt;  color: #666666;&lt;br&gt;  margin-top: 2px;&lt;br&gt;}" style="text;html=1;strokeColor=#ff9800;fillColor=#fff3e0;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=8;spacingTop=5;spacingLeft=8;fontFamily=Courier New;" vertex="1" parent="1">
